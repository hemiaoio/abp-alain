{
  "name": "abp-alain",
  "version": "0.0.0",
  "license": "MIT",
  "description": "ng-zorro-antd admin panel front-end framework",
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">= 3.0.0"
  },
  "private": true,
  "scripts": {
    "precommit": "npm run lint-staged",
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build --prod --build-optimizer",
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
    "lint:tstest": "tslint -p src/tsconfig.spec.json -c tslint.json 'src/**/*.ts'",
    "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "e2e": "ng e2e",
    "test": "ng test --watch",
    "fix-v6": "node fix-v6",
    "test-coverage": "ng test --code-coverage --watch=false"
  },
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@antv/data-set": "^0.8.9",
    "@antv/g2": "^3.1.0",
    "@antv/g2-plugin-slider": "^2.0.2",
    "@delon/abc": "^1.0.5",
    "@delon/acl": "^1.0.5",
    "@delon/auth": "^1.0.5",
    "@delon/cache": "^1.0.5",
    "@delon/form": "^1.0.5",
    "@delon/mock": "^1.0.5",
    "@delon/theme": "^1.0.5",
    "@delon/util": "^1.0.5",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "ajv": "^6.5.0",
    "core-js": "^2.5.7",
    "date-fns": "^1.29.0",
    "file-saver": "^1.3.8",
    "ng-zorro-antd": "^0.7.1",
    "ngx-countdown": "^3.0.1",
    "ngx-tinymce": "^2.0.1",
    "ngx-ueditor": "^2.1.0",
    "qrious": "^4.0.2",
    "rxjs": "^6.2.0",
    "rxjs-compat": "^6.2.0",
    "screenfull": "^3.3.2",
    "simple-line-icons": "^2.4.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.3",
    "@angular-devkit/build-angular": "~0.6.7",
    "typescript": "~2.7.2",
    "@angular/cli": "~6.0.7",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.5",
    "@types/jszip": "^3.1.4",
    "@types/mockjs": "^1.0.0",
    "codecov": "^3.0.2",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.3",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "tslint-language-service": "^0.9.9",
    "editorconfig-tools": "^0.1.1",
    "husky": "^0.14.3",
    "gh-pages": "^1.1.0",
    "lint-staged": "^7.1.3",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^1.12.1",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "webpack-bundle-analyzer": "^2.12.0",
    "xlsx": "^0.12.13",
    "less": "~2.7.3",
    "ng-alain": "^1.0.5"
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "*.ts": [
      "npm run lint:ts",
      "prettier --write",
      "git add"
    ],
    "src/**/*.less": [
      "npm run lint:style",
      "prettier --write",
      "git add"
    ]
  }
}
